{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "components/charts/BarChart.tsx",
    "components/charts/BarChart.tsx",
    "app/page.tsx",
    "components/charts/BarChart.tsx",
    "app/crops/page.jsx",
    "components/footer.jsx",
    "components/footer.jsx",
    "components/footer.jsx",
    "app/layout.jsx",
    "components/Header.jsx"
, "app/api/crops/[biologicalName/route.js", "components/crops.jsx", "components/weather/daily.jsx", "app/crops/[id]/page.jsx"  ],
  "exclude": ["node_modules"]
}
